{
  "manifest_version": 2,
  "name": "FB Scraper",
  "version": "1.0",
  "description": "",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://mbasic.facebook.com/satuk*",
        "*://mbasic.facebook.com/MemeidaObscura*",
        "*://mbasic.facebook.com/profile/timeline/stream/*",
        "*://*.fbcdn.net/*",
        "*://www.facebook.com/photo*"
      ],
      "js": [
        "scraper.js"
      ]
    }
  ]
}